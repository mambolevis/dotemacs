@article{mandelbaum2013a,
  title = {Against {{Alief}}},
  volume = {165},
  abstract = {This essay attempts to clarify the nature and structure of aliefs. First I distinguish between a robust notion of aliefs and a deflated one. A robust notion of aliefs would introduce aliefs into our psychological ontology as a hitherto undiscovered kind, whereas a deflated notion of aliefs would identify aliefs as a set of pre-existing psychological states. I then propose the following dilemma: one the one hand, if aliefs have propositional content, then it is unclear exactly how aliefs differ from psychological states we already countenance, in which case there is no robust notion of aliefs; on the other, if aliefs just contain associative content, then they cannot do the explanatory work set out for them, in which case there is no reason to posit aliefs at all. Thus, it appears that we have little reason to posit the novel category of robust aliefs.},
  timestamp = {2016-10-14T18:58:10Z},
  number = {1},
  journaltitle = {Philosophical Studies},
  author = {Mandelbaum, Eric},
  date = {2013},
  pages = {197--211},
  keywords = {Alief,Association,belief,epistemology,ethics,inference,Philosophy of Language,Philosophy of Mind,propositional attitudes,Propositional structure,rationality},
  file = {mandelbaum2013a_against_alief.pdf:/MasterLib/mandelbaum2013a_against_alief.pdf:application/pdf}

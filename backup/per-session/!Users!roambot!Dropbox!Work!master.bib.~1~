@book{chalmers2010,
  location = {{Oxford}},
  title = {The {{Character}} of {{Consciousness}}},
  abstract = {What is consciousness? How does the subjective character of consciousness fit into an objective world? How can there be a science of consciousness? In this sequel to his groundbreaking and controversial The Conscious Mind, David Chalmers develops a unified framework that addresses these questions and many others. Starting with a statement of the “hard problem” of consciousness, Chalmers builds a positive framework for the science of consciousness and a nonreductive vision of the metaphysics of consciousness. He replies to many critics of The Conscious Mind, and then develops a positive theory in new directions. The book includes original accounts of how we think and know about consciousness, of the unity of consciousness, and of how consciousness relates to the external world. Along the way, Chalmers develops many provocative ideas: the “consciousness meter”, the Garden of Eden as a model of perceptual experience, and The Matrix as a guide to the deepest philosophical problems about consciousness and the external world.},
  timestamp = {2016-10-14T19:04:00Z},
  publisher = {{Oxford University Press}},
  author = {Chalmers, David J},
  date = {2010},

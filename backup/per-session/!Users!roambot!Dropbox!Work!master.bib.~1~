@article{dyck2014b,
  title = {Beyond the {{Paralogisms}}: {{Kant}} on the {{Soul}}'s {{Immortality}} in the {{Lectures}} on {{Metaphysics}}},
  abstract = {Considered in light of the reader’s expectation of a thoroughgoing criticism of the pretensions of the rational psychologist, and of the wealth of discussions available in the broader 18th century context, which includes a variety of proofs that do not explicitly turn on the identification of the soul as a simple substance, Kant’s discussion of immortality in the Paralogisms falls lamentably short. However, outside of the Paralogisms (and the published works generally), Kant had much more to say about the arguments for the soul’s immortality as he devoted considerable time to the topic throughout his career in his lectures on metaphysics. In fact, as I show in this paper, the student lecture notes prove to be an indispensable supplement to the treatment in the Paralogisms, not only for illuminating Kant’s criticism of the rational psychologist’s views on the immortality of the soul, but also in reconciling this criticism with Kant’s own positive claims regarding certain theoretical proofs of immortality.},
  timestamp = {2016-10-14T18:52:39Z},
  author = {Dyck, Corey W},
  date = {2014},

@article{longuenesse2007,
  title = {Kant on the {{Identity}} of {{Persons}}},
  volume = {107},
  abstract = {According to Kant, the rationalist notion of a person as a thinking substance, conscious of its own identity through time, trades on an ambiguity concerning the meaning of `being conscious of the numerical identity of oneself at different times'. I argue that against the rationalist notion, Kant endorses the notion of a person as a spatio-temporal entity endowed with unity of apperception and capable of knowing its own identity through time according to empirical criteria of identification and re-identification. Nevertheless, Kant maintains that the rationalist notion is both `necessary and sufficient for practical use'. I argue that in fact, Kant's empirical notion of a person was sufficient even for the purposes of his moral philosophy. I conclude by comparing my analysis of the strengths and weaknesses of Kant's view with Peter Strawson's analysis of Kant's argument in the Paralogisms of Pure Reason.},
  timestamp = {2016-10-14T19:05:36Z},
  issue = {1, part 2},
  journaltitle = {Proceedings of the Aristotelian Society},
  author = {Longuenesse, Béatrice},
  date = {2007},
  pages = {149--167},
  keywords = {first person,kant,self-consciousness,self-knowledge},
  file = {longuenesse2007_kant_on_the_identity_of_persons.pdf:/MasterLib/longuenesse2007_kant_on_the_identity_of_persons.pdf:application/pdf}
}

@book{zuckert2007,
  title = {Kant on {{Beauty}} and {{Biology}}: {{An Interpretation}} of the `{{Critique}} of {{Judgment}}'},
  abstract = {Kant's Critique of Judgment has often been interpreted by scholars as comprising separate treatments of three uneasily connected topics: beauty, biology, and empirical knowledge. Rachel Zuckert's book interprets the Critique as a unified argument concerning all three domains. She argues that on Kant's view, human beings demonstrate a distinctive cognitive ability in appreciating beauty and understanding organic life: an ability to anticipate a whole that we do not completely understand according to preconceived categories. This ability is necessary, moreover, for human beings to gain knowledge of nature in its empirical character as it is, not as we might assume it to be. Her wide-ranging and original study will be valuable for readers in all areas of Kant's philosophy.},
  timestamp = {2016-10-14T19:05:37Z},
  publisher = {{Cambridge University Press}},
  author = {Zuckert, Rachel},
  date = {2007},
  keywords = {1724-1804,Aesthetics,concepts,judgment,kant,teleology},

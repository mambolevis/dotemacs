  abstract = {There are many kinds of scientific failure such as experimental results which are incompatible with or unexplained by scientific theory; the specific invalidation of a given hypothesis; and the abandonment of a promising theory. Yet these are not the kinds of.},
  timestamp = {2016-10-14T19:32:51Z},
  journaltitle = {Scientific failure},

@article{berman1974,
  title = {Francis {{Hutcheson}} on {{Berkeley}} and the {{Molyneux Problem}}},
  volume = {74},
  abstract = {This paper presents an unnoticed early answer to the Molyneux problem by Francis Hutcheson, the exponent of the moral sense. Hutcheson's affirmative answer to the problem-contained in a letter published in the European Magazine in 1788-follows some brief remarks on Berkeley's immaterialism, and throws light on Hutcheson's relationship with Berkeley. Hutcheson's positive answer to the problem is seen, at least in part, as a reaction against Berkeley's negative answer, and as surprisingly similar to the positive answer offered by Leibniz.},

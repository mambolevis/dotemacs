@incollection{jauernig2008,
  location = {{Princeton, NJ}},
  title = {Kant's {{Critique}} of the {{Leibnizian Philosophy}} : {{Contra}} the {{Leibnizians}}, but {{Pro Leibniz}}},
  abstract = {It is argued that the popular story that portrays Kant's philosophical development as a gradual emancipation from his Leibniz-Wolffian roots that culminated in a total rejection of the Leibnizian philosophy by 1781 is not accurate. Kant's many objections against the Leibnizian philosophy in the critical period are not directed against Leibniz himself but against the Leibniz-Wolffians. Kant considers Leibniz's philosophy to be very close to his own, calling the Critique of Pure Reason the “true apology” of Leibniz. It is claimed that this assessment is correct; the correctness is demonstrated with respect to Kant's and Leibniz's theories of space.},
  timestamp = {2016-10-14T19:05:10Z},

@article{aquila2004,
  title = {The {{Singularity}} and the {{Unity}} of {{Transcendental Consciousness}} in {{Kant}}},
  volume = {30},
  abstract = {Transcendental consciousness is described by Kant as “the one single thing” in which “as in the transcendental subject, our perceptions must be encountered.” The unity of that subject depends on intellectual functions. I argue that its singularity is just the same as that of Kant's pre-intellectual “form” of spatiotemporal “intuition.” This may seem excluded by Kant's claim that it is through intellect that “space or time are first given as intuitions.” But while pre-intellectual form is insufficient for space and time as distinct “things,” it is sufficient for the constitution of a “single thing” indifferently construable as both. Contrary to what are typically seen as the main differences between Kant and Hume on identity of “self,” there is thus already a difference in play below the level of either's concern with the sorts of connections available for the combining, or illusion of combining, of manifolds of “impressions” or “ideas.”},
  timestamp = {2016-10-14T19:20:45Z},
  number = {3},
  journaltitle = {History of European Ideas},
  author = {Aquila, Richard E},
  date = {2004},
  pages = {349--376},
  keywords = {apperception,kant,self-consciousness,transcendental deduction},
  file = {aquila2004_the_singularity_and_the_unity_of_transcendental_consciousness_in_kant.pdf:/MasterLib/aquila2004_the_singularity_and_the_unity_of_transcendental_consciousness_in_kant.pdf:application/pdf}
}

@article{travis2004,
  title = {The {{Silence}} of the {{Senses}}},
  volume = {113},
  abstract = {There is a view abroad on which (a) perceptual experience has (a) representational content in this sense: in it something is represented to the perceiver as so. On the view, a perceptual experience has a face value at which it may be taken, or which may be rejected. This paper argues that that view is mistaken: there is nothing in perceptual experience which makes it so that in it anything is represented as so (except insofar as the perceiver represents things to himself as so). In that sense, the senses are silent, or, in Austin's term, dumb. Perceptual experience is not as such either veridical or delusive. It may mislead, but it does not take representation to accomplish that.},
  timestamp = {2016-10-14T19:20:45Z},
  number = {449},
  journaltitle = {Mind},
  author = {Travis, Charles},
  date = {2004},
  pages = {57--94},
  keywords = {acquaintance,content,content view,experience,perception},
  file = {travis2004_the_silence_of_the_senses.pdf:/MasterLib/travis2004_the_silence_of_the_senses.pdf:application/pdf}
}


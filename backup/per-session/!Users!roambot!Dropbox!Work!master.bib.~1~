@article{schaffer2007,
  title = {From {{Nihilism}} to {{Monism}}},
  volume = {85},
  abstract = {Mereological nihilism is the view that all concrete objects are simple. Existence monism is the view that the only concrete object is one big simple: the world. I will argue that nihilism culminates in monism. The nihilist demands the simplest sufficient ontology, and the monist delivers it. Nothing is cheaper and commoner in philosophy than monism; what, unhappily, is still rare, is an attempt to defend it, and critically to establish its assumptions. [Schiller 1897: 62]},

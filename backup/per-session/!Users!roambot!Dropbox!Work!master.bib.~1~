@book{fine1993,
  location = {{Oxford}},
  title = {On {{Ideas}}: {{Aristotle}}'s {{Criticism}} of {{Plato}}'s {{Theory}} of {{Forms}}},
  abstract = {The Peri Ideon is the only work in which Aristotle systematically sets out and criticizes arguments for the existence of Platonic forms. Gail Fine presents the first full-length treatment in English of this important but neglected work, paying careful attention to the details of the text. She also provides a general discussion of Plato's theory of forms, and of our evidence about the Peri Ideon and its date, scope, and aims. - ;The Peri Ideon is the only work in which Aristotle systematically sets out and criticizes arguments for the existence of Platonic forms. Gail Fine presents the first full-length treatment in English of this important but neglected work. She asks how, and how well, Aristotle understands Plato's theory of forms, and why and with what justification he favours an alternative metaphysical scheme. She examines the significance of the Peri Ideon for some central questions about Plato's theory of forms - whether, for example, there are forms corresponding to every property or only to some, and if only to some, then to which ones; whether forms are universals, particulars or both; and whether they are meanings, properties or both. In addition to discussing the Peri Ideon and its sources in Plato's dialogues, Professor Fine also provides a general discussion of Plato's theory of forms, and of our evidence about the Peri Ideon and its date, scope, and aims. While she pays careful attention to the details of the text, she also relates it to contemporary philosophical concerns. The book will be valuable for anyone interested in metaphysics ancient or modern. -},
  timestamp = {2016-10-14T19:33:17Z},
  publisher = {{Oxford University Press}},
  author = {Fine, Gail},
  date = {1993},
  keywords = {Aristotle,Plato,platonism,properties,universals},
  file = {fine1993_on_ideas.pdf:/MasterLib/fine1993_on_ideas.pdf:application/pdf}
}


@article{baumann2016,
  title = {Kant, {{Neo}}-{{Kantians}}, and {{Transcendental Subjectivity}}},
  issn = {1468-0378},
  url = {http://onlinelibrary.wiley.com/doi/10.1111/ejop.12162/abstract},
  doi = {10.1111/ejop.12162},
  abstract = {This article discusses an interpretation of Kant's conception of transcendental subjectivity, which manages to avoid many of the concerns that have been raised by analytic interpreters over this doctrine. It is an interpretation put forward by selected C19 and early C20 neo-Kantian writers. The article starts out by offering a neo-Kantian interpretation of the object as something that is constituted by the categories and that serves as a standard of truth within a theory of judgment (I). The second part explicates transcendental subjectivity as the system of categories, which is self-referential and constitutes objects (II), in order to then evaluate this conception by means of a comparison with Hegel's absolute subject (III). Rather than delineating the differences between neo-Kantian writers, the article systematically expounds a shared project, which consists in providing the ultimate foundation for judgments by means of an anti-psychologist and non-metaphysical interpretation of transcendental subjectivity.},
  timestamp = {2016-10-26T19:09:46Z},
  langid = {english},
  journaltitle = {European Journal of Philosophy},
  shortjournal = {European Journal of Philosophy},
  author = {Baumann, Charlotte},
  urldate = {2016-10-26},
  date = {2016-01-01},
  pages = {n/a--n/a},
  note = {00000},
  keywords = {german idealism,german philosophy,kant,subjectivity,toread},
  file = {baumann2016_kant,_neo-kantians,_and_transcendental_subjectivity.pdf:/MasterLib/baumann2016_kant,_neo-kantians,_and_transcendental_subjectivity.pdf:application/pdf;Snapshot:/Users/roambot/Databases/Zotero_Database/storage/CF9Q58QN/abstract.html:text/html}

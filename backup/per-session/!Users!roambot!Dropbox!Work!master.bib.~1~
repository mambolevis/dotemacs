@incollection{brittan1986,
  title = {Kant’s {{Two Grand Hypotheses}}},
  rights = {©1986 D. Reidel Publishing Company},
  isbn = {978-90-277-2310-9 978-94-009-4730-6},
  url = {http://0-link.springer.com.library.unl.edu/chapter/10.1007/978-94-009-4730-6_3},
  abstract = {The second chapter of MAN, “Metaphysical Foundations of Dynamics”, is both the longest and the most difficult in the book. It is difficult not so much because what Kant says there is obscure, which it is in places, as because it is the locus of certain tensions in his thinking. Some of these tensions resulted from an incomplete understanding of the concept of continuity and a consequent groping, typical of Kant’s generation, for a satisfactory foundation for the infinitesimal calculus. Other tensions derive, I believe, from conflicts that characterize his philosophical position. I will try to make the nature of these conflicts clear in what follows. Although some of the discussion is of no more than historical interest, the problems discussed arise inevitably in the attempt to work out an adequate conception of modern science.},
  timestamp = {2016-11-03T20:25:13Z},
  langid = {english},
  number = {33},
  booktitle = {Kant’s {{Philosophy}} of {{Physical Science}}},
  series = {The Western Ontario Series in Philosophy of Science},
  publisher = {{Springer Netherlands}},
  author = {Brittan, Gordon G.},
  editor = {Butts, Robert E.},
  urldate = {2016-11-03},

@article{baxter1995,
  title = {Corporeal {{Substances}} and {{True Unities}}},
  volume = {27},
  abstract = {In seiner Korrespondenz mit Arnauld behauptet Leibniz, da$\backslash$s s jede körperliche Substanz eine substantielle Form hat. Zur Erhärtung dieser These vertritt er die Ansicht, da$\backslash$s s eine körperliche Substanz, um wirklich zu sein, eins und unteilbar sein mu$\backslash$s s, also eine echte Einheit. Ich werde zeigen, da$\backslash$s s diese Behauptung die verführerische Deutung der körperlichen Substanzen als zusammengesetzte Einheiten ausschlie$\backslash$s st. Vielmehr führt sie zu der Interpretation, da$\backslash$s s jede körperliche Substanz eine einzelne Monade ist. Die Argumentation lautet dann kurz: Alles Teilbare besteht aus Teilen, also ist es keine Einheit, also ist es nicht wirklich. Körperliche Substanzen sind wirklich. Also sind körperliche Substanzen nicht teilbar. Alles Materielle ist teilbar. Also sind körperliche Substanzen nicht materiell. Sodann behaupte ich, da$\backslash$s s die Belege aus den Texten, wonach körperliche Substanzen aus Teilen bestehen, unter Berücksichtigung verschiedener spezieller Bedeutungen des Wortes Teil, die Leibniz nutzt, verstanden werden müssen. Schlie$\backslash$s slich rege ich die Deutung an, da$\backslash$s s Leibniz, wenn er sagt, Materie sei ein Phänomen, meint, da$\backslash$s s sie fälschlicher-, aber nützlicherweise für real gehalten wird. Aus dieser Interpretation ergibt sich ein aristotelischer Begriff der körperlichen Substanz.},
  timestamp = {2016-10-14T19:32:39Z},
  number = {2},
  journaltitle = {Studia Leibnitiana},
  author = {Baxter, Donald},
